    "eslint-fix": "eslint \"_includes/**/*.js\" --fix",
    "stylelint": "stylelint --syntax scss \"_sass/**/*.scss\"",
    "stylelint-fix": "stylelint --syntax scss \"_sass/**/*.scss\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vardy/blog.git"
  },
  "author": "Jarred Vardy, Tian Qi",
